# ============ Paymob Payment Gateway Keys ============

# API Key - Found under Dashboard -> Settings -> Account Info -> API Key
PAYMOB_API_KEY=your_paymob_api_key_here

# Secret Key - From Settings inside merchant account
PAYMOB_SECRET_KEY=your_paymob_secret_key_here

# URLs
PAYMOB_FETCH_AUTH_TOKEN_URL=https://accept.paymob.com/api/auth/tokens
PAYMOB_ORDER_CREATION_URL=https://accept.paymob.com/api/ecommerce/orders
PAYMOB_FETCH_PAYMENT_REQUEST_URL=https://accept.paymob.com/api/acceptance/payment_keys
PAYMOB_WALLET_PAYMENT_URL=https://accept.paymob.com/api/acceptance/payments/pay
PAYMOB_INTENT_URL=https://accept.paymob.com/v1/intention/

# Integration IDs
PAYMOB_ONLINE_CARD_IFRAME_ID=your_online_card_iframe_id
PAYMOB_INTEGRATION_ONLINE_CARD_ID=your_integration_online_card_id
PAYMOB_INTEGRATION_MOBILE_WALLET_ID=your_integration_mobile_wallet_id

# Profile ID
PAYMOB_PROFILE_ID=your_profile_id

# Ngrok URL (for testing)
NGROK_URL=your_ngrok_url_here

# ============ Stripe Payment Gateway Keys ============

# Publishable Key (for client-side)
STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here

# Secret Key (for server-side only - keep secure)
STRIPE_SECRET_KEY=your_stripe_secret_key_here

# Stripe API Base URL
STRIPE_BASE_URL=https://api.stripe.com/v1

# Stripe Endpoints
STRIPE_CREATE_USER_URL=https://api.stripe.com/v1/customers
STRIPE_EPHEMERAL_KEYS_URL=https://api.stripe.com/v1/ephemeral_keys
STRIPE_PAYMENT_INTENT_URL=https://api.stripe.com/v1/payment_intents

# Stripe Customer ID (example)
STRIPE_CUSTOMER_ID=your_stripe_customer_id_here

# ============ PayPal Payment Gateway Keys ============

# PayPal Client ID
PAYPAL_CLIENT_ID=your_paypal_client_id_here

# PayPal Secret Key
PAYPAL_SECRET_KEY=your_paypal_secret_key_here

# PayPal Redirect URLs
PAYPAL_SUCCESS_URL=https://example.com/success
PAYPAL_CANCEL_URL=https://example.com/cancel

# ============ Future Payment Gateways ============

# Tap Integration
TAP_PUBLISHABLE_KEY=your_tap_publishable_key_here
TAP_SECRET_KEY=your_tap_secret_key_here